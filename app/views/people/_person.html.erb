<div id="<%= dom_id person %>">
  <p>
    <strong>Membership:</strong>
    <%= person.membership_id %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= person.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= person.last_name %>
  </p>

  <p>
    <strong>Dob:</strong>
    <%= person.dob %>
  </p>

  <p>
    <strong>Birth year:</strong>
    <%= person.birth_year %>
  </p>

</div>
